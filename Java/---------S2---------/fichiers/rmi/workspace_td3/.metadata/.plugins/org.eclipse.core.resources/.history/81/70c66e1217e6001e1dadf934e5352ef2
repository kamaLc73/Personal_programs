package beans;

public class Temperature {
	private double temp ;
	
	public Temperature() {
	}
	
	public Temperature(double temp) {
		super();
		this.temp = temp;
	}


	public double getTemp() {
		return temp;
	}
	public void setTemp(double temp) {
		this.temp = temp;
	}

	@Override
	public String toString() {
		return "Temperature [temp=" + temp + "]";
	}
	
}
