package rmi;

import java.rmi.Remote;
import java.rmi.RemoteException;
import beans.*;

public interface RemoteHotelReservertion extends Remote{
	public int reserver(HotelReservation hr) throws RemoteException;
	public void annuler(HotelReservation hr) throws RemoteException;
	public HotelReservation lister(int id) throws RemoteException;
	
}
 