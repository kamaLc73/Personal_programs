<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>gds</display-name>

  <servlet>
    <servlet-name>ps</servlet-name>
    <servlet-class>com.jee.web.ProduitServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ps</servlet-name>
    <url-pattern>*.Produit</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>cs</servlet-name>
    <servlet-class>com.jee.web.ClientServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cs</servlet-name>
    <url-pattern>*.Client</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>cmds</servlet-name>
    <servlet-class>com.jee.web.CommandeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cmds</servlet-name>
    <url-pattern>*.Commande</url-pattern>
  </servlet-mapping>

</web-app>